{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [{ "type": "header", "key": "accept", "value": "text/html" }],
      "destination": "/index.html"
    }
  ],
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": false }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
} 